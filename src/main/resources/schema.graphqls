type Query{
    bookById(id:ID): Book
    posts: [Post!]
    books: [Book!]
}
type Mutation{
    createPost(date:String,Title:String,body:String):Post!
}

type Post{
id:ID
date:String
title:String
body:String
}
type Book{
  id:ID
   name:String
    pageCount:Int
    author:Author
}
type Author{
   id:ID
   firstname:String
    lastname:String
}

